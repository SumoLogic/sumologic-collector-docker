{
  "api.version": "v1",
  "sources": [
    {
      "sourceType" : "LocalFile",
      "name": "app-host-file-log",
      "pathExpression": "/host/log/apps/*/*.log",
      "multilineProcessingEnabled": true,
      "automaticDateParsing": true,
      "forceTimeZone": false,
      "category": "SUMO_CATEGORY_PLACEHOLDER"
    },
    {
      "sourceType" : "LocalFile",
      "name": "app-nomad-std-log",
      "pathExpression": "/host/log/nomad/*/alloc/logs/*.current",
      "multilineProcessingEnabled": true,
      "automaticDateParsing": true,
      "forceTimeZone": false,
      "category": "SUMO_CATEGORY_PLACEHOLDER"
    },
    {
      "sourceType" : "LocalFile",
      "name": "app-nomad-file-log",
      "pathExpression": "/host/log/nomad/*/alloc/logs/*.log",
      "multilineProcessingEnabled": true,
      "automaticDateParsing": true,
      "forceTimeZone": false,
      "category": "SUMO_CATEGORY_PLACEHOLDER"
    },
    {
      "sourceType": "Syslog",
      "name": "syslog-tcp",
      "port": 514,
      "protocol": "TCP",
      "encoding": "UTF-8",
      "forceTimeZone": false,
      "category": "SUMO_CATEGORY_PLACEHOLDER"
    },
    {
      "sourceType": "Syslog",
      "name": "syslog-udp",
      "port": 514,
      "protocol": "UDP",
      "encoding": "UTF-8",
      "forceTimeZone": false,
      "category": "SUMO_CATEGORY_PLACEHOLDER"
    }
  ]
}
