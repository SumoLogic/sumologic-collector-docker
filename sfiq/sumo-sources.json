{
  "api.version": "v1",
  "sources": [
    {
      "sourceType" : "LocalFile",
      "name": "app-host-file-log",
      "pathExpression": "/host/log/apps/*/*.log",
      "multilineProcessingEnabled": true,
      "automaticDateParsing": true,
      "forceTimeZone": false,
      "category": "ap/default"
    },
    {
      "sourceType" : "LocalFile",
      "name": "app-nomad-std-log",
      "pathExpression": "/host/log/nomad/*/alloc/logs/*.std*.0",
      "multilineProcessingEnabled": true,
      "automaticDateParsing": true,
      "forceTimeZone": false,
      "category": "ap/default"
    },
    {
      "sourceType" : "LocalFile",
      "name": "app-nomad-file-log",
      "pathExpression": "/host/log/nomad/*/*.log",
      "multilineProcessingEnabled": true,
      "automaticDateParsing": true,
      "forceTimeZone": false,
      "category": "ap/default"
    },
    {
      "sourceType": "Syslog",
      "name": "syslog-tcp",
      "port": 514,
      "protocol": "TCP",
      "encoding": "UTF-8",
      "forceTimeZone": false,
      "category": "ap/default"
    },
    {
      "sourceType": "Syslog",
      "name": "syslog-udp",
      "port": 514,
      "protocol": "UDP",
      "encoding": "UTF-8",
      "forceTimeZone": false,
      "category": "ap/default"
    }
  ]
}
