{
  "api.version": "v1",
  "sources": [
    {
      "sourceType" : "LocalFile",
      "name": "host-agent-log",
      "pathExpression": "/logs/host/agent/*/current",
      "multilineProcessingEnabled": true,
      "automaticDateParsing": true,
      "forceTimeZone": false,
      "category": "SUMO_CATEGORY_PLACEHOLDER"
    },
    {
      "sourceType" : "LocalFile",
      "name": "host-app-log",
      "pathExpression": "/logs/host/apps/*.log",
      "multilineProcessingEnabled": true,
      "automaticDateParsing": true,
      "forceTimeZone": false,
      "category": "SUMO_CATEGORY_PLACEHOLDER"
    },
    {
      "sourceType" : "LocalFile",
      "name": "app-log",
      "pathExpression": "SUMO_MONITOR_PATH_PLACEHOLDER",
      "multilineProcessingEnabled": true,
      "automaticDateParsing": true,
      "forceTimeZone": false,
      "category": "SUMO_CATEGORY_PLACEHOLDER"
    },
    {
      "sourceType": "Syslog",
      "name": "syslog-tcp",
      "port": 514,
      "protocol": "TCP",
      "encoding": "UTF-8",
      "forceTimeZone": false,
      "category": "SUMO_CATEGORY_PLACEHOLDER"
    },
    {
      "sourceType": "Syslog",
      "name": "syslog-udp",
      "port": 514,
      "protocol": "UDP",
      "encoding": "UTF-8",
      "forceTimeZone": false,
      "category": "SUMO_CATEGORY_PLACEHOLDER"
    }
  ]
}
